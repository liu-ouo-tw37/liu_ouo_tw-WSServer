{
  "name": "liu_ouo_tw-wsserver",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "keywords": [],
  "author": "liu_ouo_tw",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.13.2",
    "nodejs-websocket": "^1.7.2"
  },
  "build": {
    "appId": "com.liu.wsserver",
    "productName": "Minecraft_WSServer",
    "win": {
      "target": ["nsis"],
      "icon": "assets/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true
    },
    "extraResources": [
      {
        "from": "music/",
        "to": "music/",
        "filter": ["**/*"]
      },
      {
        "from": "commands/",
        "to": "commands/",
        "filter": ["**/*"]
      },
      "math_db.json"
    ]
  }
}
